<table border="1" style="width:100%">
 <th>
 	<td>Dog Name</td>
 	<td>Dog Age</td>
 </th>
 <% @dogs.each do |dog| %>
 <tr>
 	<td><%= link_to dog.id, show_dog_path(dog.id) %></td>
 	<td><%= dog.name %></td>
 	<td><%= dog.age %></td>
 </tr>
 <% end %>
</table>
<%= link_to 'Create a new dog', new_dog_path %>